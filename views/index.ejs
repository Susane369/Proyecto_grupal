<body>
    <div class="header">
        <div class="container">
          <h1>Bienvenido a la p√°gina</h1>
        </div>
      </div>
    
      <main class="container">
        <div class="message-container">
          <% if (showMessage) { %>
            <div class="message message-dynamic"><%= message %></div>
          <% } else { %>
            <div class="message message-default">Ese es un mensaje por defecto</div>
          <% } %>
        </div>
    
        <div class="items-container">
          <h2 class="items-title">Nuestros Items</h2>
          <ul class="items-list">
            <% for (let i = 0; i < items.length; i++) { %>
              <li class="item-card"><%= items[i] %></li>
            <% } %>
          </ul>
        </div>
      </main>
